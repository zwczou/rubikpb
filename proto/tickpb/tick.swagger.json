{
  "swagger": "2.0",
  "info": {
    "title": "TICK服务",
    "version": "v1.0.0"
  },
  "tags": [
    {
      "name": "TickService"
    }
  ],
  "schemes": [
    "http"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/exchanges": {
      "get": {
        "summary": "获取支持的交易所",
        "operationId": "TickService_ListExchange",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/tickpbListExchangeResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "TickService"
        ]
      }
    },
    "/v1/subscribe": {
      "get": {
        "summary": "订阅更新",
        "operationId": "TickService_Subscribe",
        "responses": {
          "200": {
            "description": "A successful response.(streaming responses)",
            "schema": {
              "type": "object",
              "properties": {
                "result": {
                  "$ref": "#/definitions/tickpbSubscribeResponse"
                },
                "error": {
                  "$ref": "#/definitions/rpcStatus"
                }
              },
              "title": "Stream result of tickpbSubscribeResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "op",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "topics",
            "in": "query",
            "required": false,
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          }
        ],
        "tags": [
          "TickService"
        ]
      }
    },
    "/v1/symbols": {
      "get": {
        "summary": "获取交易所交易对",
        "operationId": "TickService_ListSymbol",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/tickpbListSymbolResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "exchange",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "TickService"
        ]
      }
    }
  },
